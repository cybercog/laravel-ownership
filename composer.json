{
    "name": "cybercog/laravel-ownership",
    "description": "Laravel Ownership simplify management of Eloquent model's owner.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cybercog",
        "cog",
        "laravel",
        "eloquent",
        "trait",
        "ownership",
        "owned",
        "owner",
        "ownable",
        "author",
        "maker",
        "copyright",
        "proprietor",
        "domain",
        "creator",
        "master",
        "polymorphic",
        "morph"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "a.komarev@cybercog.su",
            "homepage": "http://ell.folio.su",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/cybercog/laravel-ownership",
    "support": {
        "email": "open@cybercog.su",
        "issues": "https://github.com/cybercog/laravel-ownership/issues",
        "wiki": "https://github.com/cybercog/laravel-ownership/wiki",
        "source": "https://github.com/cybercog/laravel-ownership",
        "docs": "https://github.com/cybercog/laravel-ownership/wiki"
    },
    "require": {
        "php": "^5.6|^7.0",
        "illuminate/database": "~5.2|~5.3|~5.4|~5.5",
        "illuminate/support": "~5.2|~5.3|~5.4|~5.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.11",
        "mockery/mockery": "^0.9.8",
        "orchestra/database": "~3.4.0",
        "orchestra/testbench": "~3.4.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Ownership\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Ownership\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cog\\Ownership\\Providers\\OwnershipServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
