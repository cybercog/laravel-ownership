{
    "name": "cybercog/laravel-ownership",
    "description": "Laravel Ownership simplify management of Eloquent model's owner.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cybercog",
        "cog",
        "laravel",
        "eloquent",
        "trait",
        "ownership",
        "owned",
        "owner",
        "ownable",
        "author",
        "maker",
        "copyright",
        "proprietor",
        "domain",
        "creator",
        "master",
        "polymorphic",
        "morph"
    ],
    "authors": [
        {
            "name": "Anton Komarev",
            "email": "anton@komarev.com",
            "homepage": "http://komarev.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://komarev.com/sources/laravel-ownership",
    "support": {
        "email": "open@cybercog.su",
        "issues": "https://github.com/cybercog/laravel-ownership/issues",
        "wiki": "https://github.com/cybercog/laravel-ownership/wiki",
        "source": "https://github.com/cybercog/laravel-ownership",
        "docs": "https://github.com/cybercog/laravel-ownership/wiki"
    },
    "require": {
        "php": "^7.1.3|^8.0",
        "illuminate/database": "^5.2|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.2|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/database": "^3.5|^4.0|^5.0|^6.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cog\\Contracts\\Ownership\\": "contracts/",
            "Cog\\Laravel\\Ownership\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cog\\Tests\\Laravel\\Ownership\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cog\\Laravel\\Ownership\\Providers\\OwnershipServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
}
